@model QuanLyTiemTra.ViewModel.MutipleData

@{
    ViewBag.Title = "Them";
    Layout = null;
}

<html>
<head>
    <meta charset="utf-8">
    <title>Thêm phiếu nhập</title>

    <link href="~/css/NguyenLieu.css" rel="stylesheet" />
</head>
<body>
    <div class="wrapper">
        <div class="title">
            Thêm Đơn Hàng
        </div>
        <form method="post" action="ThemPN">
           
            <div class="field">
                <input type="text" name="TenPhieu">
                <label>Tên phiếu</label>
            </div>
            <div class="field">
                <input type="date" name="NgayNhap"">
                <label>Ngày Nhập</label>
            </div>
            <div class="field">
                <input type="text" name="TinhTrang">
                <label>Tình trạng</label>
            </div>
           


            <div class="field">
                <input type="submit" value="Thêm">
            </div>
            <div class="signup-link">
                <a href="/PhieuNhap/PhieuNhap">Quay về</a>
            </div>
        </form>
    </div>
</body>
</html>
